{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "adjsky/jodi",
    "version": "0.5.0",
    "type": "project",
    "description": "Journal & Diary",
    "keywords": [
        "journal",
        "diary"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel-lang/common": "^6.7",
        "laravel-notification-channels/webpush": "^10.4",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "laravel/wayfinder": "^0.1.12",
        "resend/resend-laravel": "^1.0",
        "spatie/eloquent-sortable": "^4.5",
        "spatie/laravel-data": "^4.18",
        "spatie/laravel-typescript-transformer": "^2.5",
        "sqids/sqids": "^0.5.0",
        "symfony/http-client": "^8.0",
        "symfony/mailgun-mailer": "^8.0",
        "symfony/postmark-mailer": "^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.7.2",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "./vendor/bin/phpstan analyse --memory-limit=512M"
        ],
        "format": [
            "./vendor/bin/pint"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan lang:update"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
